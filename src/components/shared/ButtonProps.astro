---
export interface Props {
  btn1: any;
  btn1Link: string;
}

const { btn1, btn1Link } = Astro.props as Props;
---

<div id="btn-container" class="bg-test flex py-5 bg-">
  <button
    id="dynamic-btn"
    class="py-1 px-2 mr-4 bg- sq-styles-btn bg-lavender text-carribean"
  >
    <a href={btn1Link}>{btn1}</a>
  </button>
  <button
    id="contact-btn"
    class="py-1 px-2 bg-orange text-carribean sq-styles-btn"
  >
    <a href="/contact">contact</a>
  </button>
</div>
